package org.kh.site;
//논리 연산 : &&(and), ||(or), !(not)
public class Operation7 {
	public static void main(String[] args) {
		int a = 26;
		int b = 25;
		int c = 27;
		System.out.println("a > b && a > c : "+(a > b && a> c));
		System.out.println("a > b && b > c : "+(a > b && b> c));
		System.out.println("a < b && b < c : "+(a < b && b < c));
		System.out.println("a < b && b > c : "+(a < b && b > c));
	}

}
